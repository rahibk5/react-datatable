{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es2017", "dom"], // Add "es2017" here
      "module": "esnext",
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "outDir": "dist"
    },
    "include": ["src"]
  }
  